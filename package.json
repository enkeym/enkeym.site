{
  "name": "enkeym",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack && yarn postbuild && yarn postexport",
    "postbuild": "cross-env NODE_ENV=production next-sitemap",
    "export": "next export",
    "postexport": "cp -r public/. out/",
    "start": "next start",
    "preview": "npx serve out",
    "lint": "next lint",
    "check:links": "linkinator http://localhost:3000 --silent --timeout=3000 --recurse --retry=2 --fail --skip \"(mailto|tel):\"",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:deadcode": "knip",
    "test": "yarn analyze && yarn lint && yarn validate:manifest",
    "validate:manifest": "npx --yes webmanifest-validator ./public/site.webmanifest",
    "share": "npx cloudflared tunnel --url http://localhost:3000"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@next/bundle-analyzer": "^15.2.4",
    "@react-three/drei": "^10.0.5",
    "@react-three/fiber": "^9.1.1",
    "framer-motion": "^12.6.3",
    "next": "^16.1.3",
    "next-seo": "^6.6.0",
    "next-sitemap": "^4.2.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-intersection-observer": "^9.16.0",
    "react-type-animation": "^3.2.0",
    "three": "^0.175.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.175.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "^16.1.3",
    "knip": "^5.47.0",
    "linkinator": "^6.1.2",
    "postcss": "^8.5.3",
    "serve": "^14.2.4",
    "typescript": "^5.8.3"
  }
}
